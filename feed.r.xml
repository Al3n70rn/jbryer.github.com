---
layout: none
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
	<channel>
		<title>{{ site.name }} - R</title>
		<description>Posts categorized as 'R'</description>
		<link>{{ site.url }}</link>
		<atom:link href="{{ site.url }}/feed.r.xml" rel="self" type="application/rss+xml" />
		{% for post in site.tags.R limit:10 %}
			<item>
				<title>{{ post.title }}</title>
				<description>{{ post.content | more: "excerpt" | xml_escape }} &lt;a href='{{ site.url }}{{ post.url }}'&gt;Read full post...&lt;/a&gt;</description>
				<pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
				<link>{{ site.url }}{{ post.url }}</link>
				<guid isPermaLink="true">{{ site.url }}{{ post.url }}</guid>
			</item>
		{% endfor %}
	</channel>
</rss>
